{
    "name": "ShopSensor AI",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    },
    "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs"
    ]
}